{
	"breadcrumbs.enabled": true,
	"chatgpt.method": "Browser Auto-login",
	"chatgpt.chromiumPath": "/opt/microsoft/msedge/microsoft-edge",
	"chatgpt.gpt3.model": "text-davinci-002-render",
	"editor.bracketPairColorization.independentColorPoolPerBracketType": true,
	"editor.detectIndentation": false,
	"editor.inlineSuggest.enabled": true,
	"editor.insertSpaces": false,
	"editor.lineNumbers": "relative",
	"editor.minimap.enabled": false,
	"editor.renderWhitespace": "all",
	"editor.stickyScroll.enabled": true,
	"editor.tabSize": 2,
	"explorer.autoReveal": false,
	"files.associations": {
		"*.json": "jsonc"
	},
	"files.exclude": {
		"**/bin/": true,
		"**/obj/": true,
		"**/.vscode/": true
	},
	"git.enabled": false,
	"keyboard.dispatch": "keyCode",
	"omnisharp.enableRoslynAnalyzers": true,
	"php.validate.executablePath": "/usr/bin/php",
	"python.defaultInterpreterPath": "/bin/python3",
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		"**/bin": true,
		"**/obj": true
	},
	"vim.hlsearch": true,
	"vim.normalModeKeyBindingsNonRecursive": [
		{ "before": ["u"    ], "after": [], "commands": [ { "command": "undo" } ] },
		{ "before": ["<C-r>"], "after": [], "commands": [ { "command": "redo" } ] }
	],
	"window.commandCenter": true,
	"window.titleBarStyle": "custom",
	"workbench.colorTheme": "Solarized Dark",
	"workbench.editor.enablePreviewFromQuickOpen": false,
	"workbench.preferredDarkColorTheme": "Solarized Dark",
	"workbench.startupEditor": "newUntitledFile",
	"workbench.tree.renderIndentGuides": "always",
	"zenMode.hideLineNumbers": false,
	"vim.handleKeys": {
		"<C-d>": true,
		"<C-p>": false,
		"<C-s>": false,
		"<C-z>": false
	},
	"github.copilot.enable": {
		"*": false,
		"plaintext": false,
		"markdown": false,
		"scminput": false,
		"csharp": true,
		"aspnetcorerazor": true
	},
	"zenMode.restore": true,
	"workbench.activityBar.location": "hidden",
	"github.copilot.editor.enableAutoCompletions": true
}
